<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

   <servlet>
       <servlet-name>Start</servlet-name>
       <servlet-class>Servlet</servlet-class>
   </servlet>
    <servlet-mapping>
        <servlet-name>Start</servlet-name>
        <url-pattern>/startPage</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>FillFormServlet</servlet-name>
        <servlet-class>FillFormServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FillFormServlet</servlet-name>
        <url-pattern>/secondPage</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>StartPage</servlet-name>
        <jsp-file>/StartPage.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>StartPage</servlet-name>
        <url-pattern>/gavangard</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>FillForm</servlet-name>
        <jsp-file>/FillForm.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>FillForm</servlet-name>
        <url-pattern>/fillForm</url-pattern>
    </servlet-mapping>

</web-app>